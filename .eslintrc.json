{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["airbnb-base", "prettier"],
  "overrides": [],
  "plugins": ["prettier", "import"],
  "rules": {
    "prettier/prettier": "off",
    "no-unused-vars": "warn",
    "no-param-reassign": "off",
    "func-names": "off"
  },
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  }
}
